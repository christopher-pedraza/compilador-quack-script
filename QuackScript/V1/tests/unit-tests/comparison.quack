// Prueba: InstrucciÃ³n print con varias expresiones y cadenas
program TestPrint;

main {
    // GT
    print("10 > 20: ", 10 > 20);        // 0
    print("\n20 > 10: ", 20 > 10);      // 1

    // LT
    print("\n\n10 < 20: ", 10 < 20);      // 1
    print("\n20 < 10: ", 20 < 10);      // 0

    // NE
    print("\n\n10 != 20: ", 10 != 20);    // 1
    print("\n20 != 20: ", 20 != 20);    // 0

    // EE
    print("\n\n10 == 20: ", 10 == 20);    // 0
    print("\n20 == 20: ", 20 == 20);    // 1

    // GTE
    print("\n\n10 >= 20: ", 10 >= 20);    // 0
    print("\n20 >= 10: ", 20 >= 10);    // 1

    // LTE
    print("\n\n10 <= 20: ", 10 <= 20);    // 1
    print("\n20 <= 10: ", 20 <= 10);    // 0


}
end