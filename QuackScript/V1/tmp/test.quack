// Prueba: Factorial iterativo y recursivo

program Factorial;

int factorial_iterativo(n: int) [
    var result, i: int;
    {
        if (n < 0) {
            return -1;
        };
        result = 1;
        i = 2;

        while (i <= n) do {
            result = result * i;
            i = i + 1;
        };
    }
];

main {
    print("Factorial iterativo: ", factorial_iterativo(5), "\n");
}
end